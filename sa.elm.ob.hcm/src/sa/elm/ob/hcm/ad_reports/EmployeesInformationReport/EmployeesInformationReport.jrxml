	<queryString>
		<![CDATA[select emp.value as empno,emp.arabicfullname as empname,empinfomax.changereason,eut_convert_to_hijri(to_char(empinfosd.startdate,'YYYY-MM-DD')) as startdate,
n.name as nationality,pos.job_no as position,eut_convert_to_hijri(to_char(emp.govhiredate,'YYYY-MM-DD')) as govhiredate,
eut_convert_to_hijri(to_char(emp.hiredate,'YYYY-MM-DD')) as hiredate,coalesce(trl.name,list.name) as qualification,
eut_convert_to_hijri(to_char(emp.Dob,'YYYY-MM-DD')) as dob,coalesce(statuslist.name,status.name) as empstatus,
ehcm_calculate_age(emp.ehcm_emp_perinfo_id,emp.ad_client_id,to_char(emp.dob,'YYYY-MM-DD'),to_char(now(),'YYYY-MM-DD')) as age,
ehcm_calculate_age(emp.ehcm_emp_perinfo_id,emp.ad_client_id,to_char(emp.startdate,'YYYY-MM-DD'),to_char(coalesce(empinfomax.enddate,now()),'YYYY-MM-DD')) as period,
grcl.name as empcategory,org.name as dept,emp.nationality_identifier as nationalidentifer,grcl.name as empcategory,gr.name as grade,
jobs.job_title as title,(jobs.value || concat('-',jobs.job_title)) as positiontitle,
case when emp.gender='M' then 'Male' when emp.gender='F' then 'Female' end as gender,payscln.amount as basicsalary,qual.degree as degree,
superemp.arabicfullname as linemanager,(select case when emp.isactive = 'N' then eut_convert_to_hijri(to_char(termin.enddate,'YYYY-MM-DD'))
 when termin.changereason='T' then eut_convert_to_hijri(to_char(termin.startdate,'YYYY-MM-DD')) end
 from ehcm_employment_info termin where  termin.ehcm_emp_perinfo_id= emp.ehcm_emp_perinfo_id
 order by created  desc limit 1 ) as endofservice,coalesce(decbal.period_of_service,0) as
period_of_service
from ehcm_emp_perinfo emp
left join
(select empinfo.changereason,empinfo.ehcm_emp_perinfo_id,empinfo.ehcm_position_id,empinfo.deptcode,empinfo.employmentgrade,empinfo.jobcode,
empinfo.ehcm_payscaleline_id,empinfo.EHCM_Emp_Supervisor_id,empinfo.enddate
from ehcm_employment_info empinfo where empinfo.created  =
(select max(info.created) from ehcm_employment_info info where info.ehcm_emp_perinfo_id = empinfo.ehcm_emp_perinfo_id))
 empinfomax on emp.ehcm_emp_perinfo_id = empinfomax.ehcm_emp_perinfo_id
left join
 (select empinfoh.startdate,empinfoh.ehcm_emp_perinfo_id from ehcm_employment_info empinfoh where empinfoh.changereason = 'H')
 empinfosd on emp.ehcm_emp_perinfo_id = empinfosd.ehcm_emp_perinfo_id
left join ehcm_position pos on pos.ehcm_position_id = empinfomax.ehcm_position_id
left join ehcm_gradeclass grcl on grcl.ehcm_gradeclass_id = emp.ehcm_gradeclass_id
left join ad_org org on org.ad_org_id = empinfomax.deptcode
left join ehcm_grade gr on gr.ehcm_grade_id=empinfomax.employmentgrade
left join Ehcm_jobs jobs on jobs.ehcm_jobs_id = empinfomax.jobcode
left join ad_ref_list listst on listst.value=emp.employment_status and listst.ad_reference_id='BD5BC7FE25C64B158971A7DCE5732DE9'
left join AD_Ref_List_Trl trlst on listst.ad_ref_list_id=trlst.ad_ref_list_id and trlst.ad_language='ar_SA'
left join ehcm_addnationality n on n.ehcm_addnationality_id = emp.ehcm_addnationality_id
left join ehcm_qualification qual on qual.ehcm_emp_perinfo_id = emp.ehcm_emp_perinfo_id
left join ad_ref_list list on list.value=qual.edulevel and list.ad_reference_id='3FA605387FD5432BB9F334A3CB7C4965'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
left join ehcm_payscaleline payscln on payscln.ehcm_payscaleline_id=empinfomax.ehcm_payscaleline_id
left join EHCM_Emp_Supervisor superv on superv.EHCM_Emp_Supervisor_id = empinfomax.EHCM_Emp_Supervisor_id
left join ehcm_emp_perinfo superemp on superemp.ehcm_emp_perinfo_id = superv.ehcm_emp_perinfo_id
left join ehcm_employeestatus_v status on status.ehcm_employeestatus_v_id= emp.ehcm_emp_perinfo_id
left join AD_Ref_List_Trl statuslist on statuslist.ad_ref_list_id= status.ad_ref_list_id and statuslist.ad_language='ar_SA'
left join ( select coalesce(decbal.period_of_service,0) as period_of_service, decbal.ehcm_emp_perinfo_id  from ehcm_decision_balance decbal join ehcm_deci_bal_hdr decbalhdr on decbalhdr.ehcm_deci_bal_hdr_id= decbal.ehcm_deci_bal_hdr_id
where decbalhdr.status='CO' and  decbal.decision_type='TPS')
decbal on decbal. ehcm_emp_perinfo_id= emp.ehcm_emp_perinfo_id

where emp.ad_client_id =  $P{inpClientId}
and case when $P{inpEmpId} is null then true else emp.ehcm_emp_perinfo_id=$P{inpEmpId} end
and case when $P{inpDepartmentId} is null then true else empinfomax.deptcode=$P{inpDepartmentId} end
and case when $P{inpEmpTypeId} is null then true else emp.ehcm_gradeclass_id=$P{inpEmpTypeId} end
and case when $P{inpEmpStatus} is null then true else status.statusvalue=$P{inpEmpStatus} end
and case when $P{inpGenderId} is null then true else emp.gender=$P{inpGenderId} end
and case when $P{inpLineManagerId} is null then true else empinfomax.EHCM_Emp_Supervisor_id = $P{inpLineManagerId} end
and empinfosd.startdate between cast($P{inpStartDate} as date) and cast($P{inpEndDate} as date)
and case when ($P{inpEmpGradeFromId}='0'  and $P{inpEmpGradeToId}='0') then true else true end
and case when ($P{inpEmpGradeFromId}='0' and $P{inpEmpGradeToId}!='0') then gr.value<=$P{inpEmpGradeToId} else true end
and case when ($P{inpEmpGradeFromId}!='0' and $P{inpEmpGradeToId}='0') then gr.value>=$P{inpEmpGradeFromId} else true end
and case when ($P{inpEmpGradeFromId}!='0' and $P{inpEmpGradeToId}!='0') then(gr.value>=$P{inpEmpGradeFromId} and gr.value<=$P{inpEmpGradeToId}) else true end order by empno asc]]>
	</queryString>
